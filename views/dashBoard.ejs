        <div class="row">
          <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
            <!-- card for money  -->
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div
                  class="icon icon-lg icon-shape bg-gradient-dark shadow-dark text-center border-radius-xl mt-n4 position-absolute"
                >
                  <i class="material-icons opacity-10">weekend</i>
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Today's Money</p>
                  <h4 class="mb-0"><%= stats.payment.today %> tk</h4>
                </div>
              </div>
              <hr class="dark horizontal my-0" />
              <div class="card-footer p-3">
                <p class="mb-0">
                  <% const perpay = ((stats.payment.tomorow -
                  stats.payment.today)/stats.payment.tomorow)*100 %> <%
                  if(perpay < 0){ %>
                  <span class="text-success text-sm font-weight-bolder"
                    ><%= Math.abs(perpay) %>%
                  </span>
                  <% }else{ %>
                  <span class="text-danger text-sm font-weight-bolder"
                    ><%= -Math.abs(perpay) %>%
                  </span>
                  <% } %> than last Day
                </p>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
            <!-- // user card count -->
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div
                  class="icon icon-lg icon-shape bg-gradient-primary shadow-primary text-center border-radius-xl mt-n4 position-absolute"
                >
                  <i class="material-icons opacity-10">person</i>
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Today's Users</p>
                  <h4 class="mb-0"><%= stats.user.today %></h4>
                </div>
              </div>
              <hr class="dark horizontal my-0" />
              <div class="card-footer p-3">
                <p class="mb-0">
                  <% const per = ((stats.user.tomorow -
                  stats.user.today)/stats.user.tomorow)*100 %> <% if(per < 0){
                  %>
                  <span class="text-success text-sm font-weight-bolder"
                    ><%= Math.abs(per) %>%
                  </span>
                  <% }else{ %>
                  <span class="text-danger text-sm font-weight-bolder"
                    ><%= -Math.abs(per) %>%
                  </span>
                  <% } %> than last Day
                </p>
              </div>
            </div>
          </div>

